<h1>@<%= @user.username %> <small>Tag collections</small></h1>
<div class="list-group">
  <% @tag_collections.each do |tag_collection| %>
    <%= link_to links_path(tags: tag_collection.tag_list_string), class: "list-group-item" do %>
      <span class="label label-primary"><%= tag_collection.tag_list_string %></span>
    <% end %>
  <% end %>
</div>
